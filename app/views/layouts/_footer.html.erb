<footer>
	<div class="wrapper wrapper_footer clearfix">
		<div class="pull-left">
			<div id="instafeed"></div>
		</div>
		<nav class="pull-right">
			<div class="social">
				<a href="http://www.instagram.com/beerdid" target="_blank" title="Beerdid Instagram"><i class="fa fa-instagram fa-lg"></i></a>
				<a href="https://www.facebook.com/beerdidofficial" target="_blank" title="Beerdid Facebook"><i class="fa fa-facebook-square fa-lg"></i></a>
				<a href="http://www.twitter.com/beerdid" target="_blank" title="Beerdid Twitter"><i class="fa fa-twitter-square fa-lg"></i></a>
			</div>
			<ul>
				<li><%= link_to "About", page_path("about") %></li>
				<li><%= link_to "Terms of Service", page_path("terms-of-service") %></li>
				<li><%= link_to "Privacy", page_path("privacy") %></li>
			</ul>
		</nav>
	</div>
</footer>

<script>
  var nav = responsiveNav(".nav-collapse",{
      jsClass: "js", // String: 'JS enabled' class which is added to  element

  });
</script>

<script type="text/javascript">
    var feed = new Instafeed({
        clientId: '82d1d4f4904648b3a1828ff35f060521',
        get: 'user',
  		userId: '1716194',
  		limit:"4",
  		accessToken: '<%= ENV["INSTAGRAM_ACCESS_TOKEN"] %>',
        template: '<a href="{{link}}"><img src="{{image}}" /></a>',
        resolution: "low_resolution"
    });
    feed.run();
</script>

</body>
</html>